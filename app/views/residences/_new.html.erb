<%= form_for(residence) do |f| %>
  <div class="field row">
    <%= f.label :郵便番号（ハイフンなし） %>
    <%= f.text_field :postcode %>
  </div>

  <div class="field row">
    <%= f.label :住所 %>
    <%= f.text_field :address%>
  </div>
    <div class="field row">
    <%= f.label :宛名 %>
    <%= f.text_field :name %>
  </div>

  <div class="actions row">
    <%= f.submit "送信" ,class: 'btn btn-primary' %>
  </div>
<% end %>
