<%= render 'admin/admin_header' %>
<div class="row" >
<div class="col-md-8 col-md-offset-2">
	<h2>会員一覧</h2>

<table class="table table-hover table-inverse">
			<thead>
				<tr>
					<th>会員ID</th>
					<th>氏名</th>
					<th>メールアドレス</th>
					<th>ステータス</th>
					<th colspan="4"></th>
				</tr>
			</thead>
            <tbody>
				<% @users.each do |user| %>
				<tr>
					<td><%= user.id %></td>

					<td><%= link_to user.last_name+user.first_name, admin_user_path(user) %></td>

					<td><%= user.email %></td>
					<td><%= user.user_status %></td>
				</tr>
				<% end %>
			</tbody>
</table>
</div>
</div>